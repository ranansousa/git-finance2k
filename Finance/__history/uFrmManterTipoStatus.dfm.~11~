  end
  object qryPreencheGrid: TFDQuery
    Connection = dmConexao.conn
    SQL.Strings = (
      'SELECT TS.DESCRICAO, TS.ID_TIPO_STATUS'
      ''
      'FROM TIPO_STATUS TS'
      ''
      ''
      'WHERE (TS.ID_ORGANIZACAO = :PIDORGANIZACAO)'
      ''
      'ORDER BY TS.DESCRICAO'
      ''
      ''
      '')
    Left = 688
    Top = 336
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end>
  end
