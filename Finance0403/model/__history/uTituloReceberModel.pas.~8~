     //coleçoes
    // exemplo FListaFormasPagto: TObjectList<TTPBaixaFPModel>; //rateio de formas
    FSlistaHistorico    : TObjectList<TTituloPagarHistoricoModel>;
    FSlistaCustos       : TObjectList<TTituloPagarCentroCustoModel>;


  public


