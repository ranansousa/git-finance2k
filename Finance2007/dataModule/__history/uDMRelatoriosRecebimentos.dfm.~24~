      '      (TR.ID_ORGANIZACAO = :PIDORGANIZACAO)AND'
      
        '      (TR.DATA_EMISSAO  BETWEEN :DTDATAINICIAL AND :DTDATAFINAL)' +
        ';')
    Left = 264
