object dmServerMail: TdmServerMail
  OldCreateOrder = False
  Height = 309
  Width = 406
  object qryObterDadosMail: TFDQuery
    Connection = dmConexao.Conn
    SQL.Strings = (
      'SELECT ID_SERVIDOR_EMAIL,HOST,REQUER_AUTENTICACAO,'
      '       REMETENTE,LOGIN,SENHA'
      ''
      'FROM SERVIDOR_EMAIL '
      ''
      'WHERE (ID_SERVIDOR_EMAIL = :pIdServidor);')
    Left = 88
    Top = 40
    ParamData = <
      item
        Name = 'PIDSERVIDOR'
        DataType = ftInteger
        ParamType = ptInput
      end>
  end
end
