object dmTRCentroCustoConsulta: TdmTRCentroCustoConsulta
  OldCreateOrder = False
  Height = 402
  Width = 858
  object qryCentroCustoTR: TFDQuery
    Connection = dmConexao.Conn
    FormatOptions.AssignedValues = [fvFmtDisplayDate, fvFmtDisplayNumeric, fvFmtEditNumeric]
    FormatOptions.FmtDisplayDate = 'DD/MM/YYYY'
    FormatOptions.FmtDisplayNumeric = '###,##0.00'
    FormatOptions.FmtEditNumeric = '###,##0.00'
    SQL.Strings = (
      'SELECT TPC.VALOR,       '
      '       TPC.ID_TITULO_RECEBER,'
      '       CC.DESCRICAO,'
      '       CC.CODIGO'
      '        '
      'FROM TITULO_RECEBER_RATEIO_CC TPC'
      
        'JOIN CENTRO_CUSTO CC ON (CC.ID_CENTRO_CUSTO = TPC.ID_CENTRO_CUST' +
        'O)'
      ''
      'WHERE    (TPC.ID_ORGANIZACAO = :pIdOrganizacao) AND'
      '         (TPC.ID_TITULO_RECEBER = :pId_TITULO_RECEBER) '
      ''
      'ORDER BY TPC.VALOR DESC;'
      ''
      '       '
      '')
    Left = 680
    Top = 210
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end
      item
        Name = 'PID_TITULO_RECEBER'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end>
  end
end
