  object qryObterEndPorID: TFDQuery
    Connection = dmConexao.Conn
    SQL.Strings = (
      'SELECT E.id_endereco,'
      '       E.ID_BAIRRO,'
      '       E.ID_CIDADE,'
      '       E.ID_ESTADO,'
      '       E.logradouro, '
      '       E.COMPLEMENTO,'
      '       E.NUMERO,'
      '       E.CEP,'
      '       B.bairro,'
      '       C.codigo,'
      '       C.cidade'
      ''
      'FROM ENDERECO E'
      'LEFT OUTER join BAIRRO B ON (B.id_bairro = E.ID_BAIRRO)'
      'LEFT OUTER JOIN CIDADE C ON (C.ID_CIDADE = B.ID_CIDADE)'
      ''
      'WHERE (E.id_organizacao = :PIDORGANIZACAO)  AND'
      '      (E.id_endereco =   :PID)'
      '      ')
    Left = 44
    Top = 464
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end
      item
        Name = 'PID'
        DataType = ftString
        ParamType = ptInput
        Size = 36
      end>
  end
