object DataModule1: TDataModule1
  OldCreateOrder = False
  Height = 211
  Width = 449
  object qryAllTables: TFDQuery
    SQL.Strings = (
      'SELECT rdb$relation_name'
      'FROM rdb$relations'
      'WHERE rdb$system_flag = 0'
      '  --  and rdb$relation_type = 0'
      'order by rdb$relation_name'
      ';')
    Left = 64
    Top = 40
  end
end
