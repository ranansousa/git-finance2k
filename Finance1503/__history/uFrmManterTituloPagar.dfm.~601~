  object qryPreencheGridCentroC: TFDQuery
    Connection = dmConexao.conn
    SQL.Strings = (
      ' SELECT C.ID_CENTRO_CUSTO  , '
      '        C.ID_ORGANIZACAO, '
      '        C.DESCRICAO , '
      '        C.SIGLA,            '
      '        C.CODIGO '
      ''
      '  FROM CENTRO_CUSTO C'
      '  WHERE (C.ID_ORGANIZACAO = :PIDORGANIZACAO)'
      '   ORDER BY C.DESCRICAO ')
    Left = 816
    Top = 93
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
