  object dsBaixas: TDataSource
    DataSet = qryBaixas
    Left = 180
    Top = 229
  end
  object qryBaixas: TFDQuery
    Connection = dmConexao.conn
    FormatOptions.AssignedValues = [fvFmtDisplayDate, fvFmtDisplayNumeric, fvFmtEditNumeric]
    FormatOptions.FmtDisplayDate = 'DD/MM/YYY'
    FormatOptions.FmtDisplayNumeric = '###,##0.00'
    FormatOptions.FmtEditNumeric = '###,##0.00'
    SQL.Strings = (
      'SELECT TPB.VALOR_PAGO AS VALOR,'
      '       TP.data_pagamento AS DATA,'
      '       TP.numero_documento AS DOC,'
      '       TP.id_historico,'
      '       TP.descricao AS DSC,'
      '       H.descricao AS DSC_HIST,'
      '       CC.descricao AS DSC_CC,'
      '       TP.id_tipo_status'
      ''
      ''
      ''
      'FROM TITULO_PAGAR_BAIXA TPB'
      
        'LEFT OUTER JOIN TITULO_PAGAR TP ON (TP.ID_ORGANIZACAO = :PIDORGA' +
        'NIZACAO AND TP.ID_TITULO_PAGAR = TPB.ID_TITULO_PAGAR )'
      
        'LEFT OUTER JOIN HISTORICO H ON    (H.ID_ORGANIZACAO  = :PIDORGAN' +
        'IZACAO AND H.ID_HISTORICO = TP.ID_HISTORICO )'
      
        'LEFT OUTER JOIN CENTRO_CUSTO CC ON    (CC.ID_ORGANIZACAO  = :PID' +
        'ORGANIZACAO AND CC.ID_CENTRO_CUSTO = TP.ID_CENTRO_CUSTO )'
      ''
      'WHERE (TP.ID_ORGANIZACAO = :PIDORGANIZACAO)'
      ' AND  (TP.ID_CEDENTE = :PIDCENTE)'
      ''
      'ORDER BY TP.DATA_PAGAMENTO DESC'
      ''
      '')
    Left = 276
    Top = 229
    ParamData = <
      item
        Name = 'PIDORGANIZACAO'
        DataType = ftString
        ParamType = ptInput
        Size = 36
        Value = Null
      end
      item
        Name = 'PIDCENTE'
        DataType = ftString
        ParamType = ptInput
        Size = 36
        Value = Null
      end>
  end
