    FFID: string;
    FFdescricao: string;
    FFIDorganizacao: string;
    FFincMF: string;
    FFtipo: string;
    FFgrau: string;
    FFrelacionamento: string;
    FFnatureza: string;

    FFdataRegistro: TDateTime;
    FFordemDIPJ: Integer;



    function getFFcodigoReduzido: string;
